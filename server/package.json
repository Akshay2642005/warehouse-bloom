{
  "name": "warehouse-server",
  "version": "1.0.0",
  "description": "Warehouse management system backend",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate",
    "db:seed": "tsx prisma/seed.ts",
  "test": "cross-env NODE_ENV=test SKIP_REDIS=1 jest --passWithNoTests",
  "test:unit": "cross-env NODE_ENV=test SKIP_REDIS=1 jest --selectProjects server-unit",
  "test:int": "cross-env NODE_ENV=test SKIP_REDIS=1 jest --selectProjects server-integration --runInBand",
  "test:coverage": "cross-env NODE_ENV=test SKIP_REDIS=1 jest --coverage --runInBand",
  "test:clean": "tsx scripts/cleanup-test-build-artifacts.ts && echo cleanup done"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "nodemailer": "^6.10.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.3",
    "redis": "^5.8.2",
    "speakeasy": "^2.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/supertest": "^2.0.16",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.20",
    "@types/pg": "^8.15.5",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "prisma": "^5.7.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "supertest": "^7.0.0",
    "cross-env": "^7.0.3"
  }
}
